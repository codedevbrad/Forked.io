enum DiscoverType {
  YOUTUBE 
  TIKTOK
  INSTAGRAM
}

model DiscoverVideo {
  id String @id @default(cuid())
  name String
  description String
  url String
  type DiscoverType
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}


model RecipeWebsites {
  id String @id @default(cuid())
  name String
  url String
  description String @default("")
  imageURL String @default("https://via.placeholder.com/150")
  logoURL String @default("https://via.placeholder.com/150")
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}