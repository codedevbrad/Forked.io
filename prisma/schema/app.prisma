enum DiscoverType {
  YOUTUBE 
  TIKTOK
  INSTAGRAM
}

model DiscoverVideo {
  id String @id @default(cuid())
  name String
  description String
  url String
  type DiscoverType
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}


model RecipeWebsites {
  id String @id @default(cuid())
  name String
  url String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}